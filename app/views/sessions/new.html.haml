- content_for :title, t('.title')
- content_for :body_id, 'page_url_login'

= simple_form_for Account.new, url: login_path do |form|
  = form.input :email, autofocus: true
  = form.input :password

  .form-group
    .col-sm-offset-3.col-md-offset-2.col-sm-9.col-md-10
      = form.button :submit, value: t('.title')

