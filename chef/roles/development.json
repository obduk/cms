{
  "name": "development",
  "description": "Sets up system for development",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "nodejs": {
      "npm_packages": [
        {
          "name": "bower",
          "version": "1.8.0"
        }
      ]
    },
    "postgresql": {
      "password": {
        "postgres": "password"
      },
      "pg_hba": [{
        "type": "host",
        "db": "all",
        "user": "all",
        "addr": "127.0.0.1/32",
        "method": "trust"
      }]
    },
    "rbenv": {
      "user_installs": [{
        "user": "vagrant",
        "rubies": ["2.3.2"],
        "global": "2.3.2",
        "gems": {
          "2.3.2": [
            { "name": "bundler" }
          ]
        }
      }]
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[imagemagick]",
    "recipe[nodejs]",
    "recipe[postgresql::server]",
    "recipe[ruby_build]",
    "recipe[ruby_rbenv::user]",
    "recipe[xvfb::package]"
  ],
  "env_run_lists": {
  }
}
